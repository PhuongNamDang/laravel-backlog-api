{
    "name": "phuongnamdang/laravel-backlog-api",
    "description": "Backlog API Library for Laravel",
    "type": "library",
    "require": {
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.1",
        "satooshi/php-coveralls": "^1.0",
        "phpunit/phpunit": "^5.7"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Đặng Phương Nam"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Itigoppo\\BacklogApi\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Itigoppo\\BacklogApi\\Test\\": "tests/TestCase"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Itigoppo\\BacklogApi\\Providers\\LaravelServiceProvider"
            ],
            "aliases": {
                "BacklogApi": "Itigoppo\\BacklogApi\\Facades\\Backlog"
            }
        }
    }
}
